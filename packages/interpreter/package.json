{
  "name": "interpreter",
  "displayName": "interpreter",
  "description": "This is the interpreter of a CCFG.",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.56.0"
  },
  "main": "InterpretCCFG.ts",
  "types": "dist/InterpretCCFG.d.ts",
  "categories": [
    "Programming Languages"
  ],
  "files": [
    ".",
    "dist"
  ],
  "exports": {
    "./api": {
      "import": "./dist/InterpretCCFG.js",
      "default": "./dist/InterpretCCFG.js",
      "types": "./dist/InterpretCCFG.d.ts"
    }
  },
  "scripts": {
        "vscode:prepublish": "npm run build && npm run lint",
        "build": "tsc -b tsconfig.json",
        "watch": "tsc -b tsconfig.json --watch",
        "lint": "eslint src --ext ts",
        "langium:generate": "langium generate",
        "langium:watch": "langium generate --watch"
    },
  "dependencies": {
    "backend-compiler": "^1.0.0",
    "ccfg": "^1.0.0",
    "chalk": "^4.1.2",
    "chevrotain": "^10.4.1",
    "commander": "^8.0.0",
    "langium": "~1.0.0",
    "vscode-languageclient": "^8.0.2",
    "vscode-languageserver": "^8.0.2",
    "vscode-uri": "^3.0.2"
  },
  "devDependencies": {
    "@types/node": "^14.17.3",
    "@types/vscode": "^1.56.0",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.17.0",
    "langium-cli": "~1.0.0",
    "typescript": "^4.9.5"
  }
}
